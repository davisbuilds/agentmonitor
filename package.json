{
  "name": "agentmonitor",
  "version": "0.1.0",
  "description": "Real-time localhost dashboard for monitoring AI agent activity",
  "author": "davisbuilds",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && cp -r src/pricing/data dist/pricing/data && pnpm run css:build",
    "start": "node dist/server.js",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts' 'scripts/**/*.ts'",
    "test": "node --import tsx --test tests/**/*.test.ts",
    "test:dead-code": "node --import tsx --test tests/codebase/dead-code.test.ts",
    "test:watch": "node --import tsx --test --watch tests/**/*.test.ts",
    "css:build": "tailwindcss -i src/input.css -o public/css/output.css --minify",
    "css:watch": "tailwindcss -i src/input.css -o public/css/output.css --watch",
    "seed": "tsx scripts/seed.ts",
    "bench:ingest": "tsx scripts/benchmark-ingest.ts",
    "recalculate-costs": "tsx scripts/recalculate-costs.ts",
    "import": "tsx scripts/import.ts",
    "rust:import": "cargo run --manifest-path rust-backend/Cargo.toml --bin import --",
    "rust:dev": "cargo run --manifest-path rust-backend/Cargo.toml --bin agentmonitor-rs",
    "rust:build": "cargo build --manifest-path rust-backend/Cargo.toml --release",
    "rust:test": "cargo test --manifest-path rust-backend/Cargo.toml",
    "rust:test:desktop-invariants": "cargo test --manifest-path rust-backend/Cargo.toml --test desktop_invariants",
    "rust:check": "cargo check --manifest-path rust-backend/Cargo.toml",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:release:mac": "bash scripts/release/macos-release.sh",
    "tauri:release:mac:unsigned": "bash scripts/release/macos-release.sh --mode unsigned",
    "tauri:release:mac:signed": "bash scripts/release/macos-release.sh --mode signed",
    "tauri:release:mac:notarized": "bash scripts/release/macos-release.sh --mode signed-notarized",
    "test:parity:ts": "AGENTMONITOR_BASE_URL=http://127.0.0.1:3141 node --import tsx --test tests/parity/**/*.test.ts",
    "test:parity:rust": "AGENTMONITOR_BASE_URL=http://127.0.0.1:3142 node --import tsx --test tests/parity/**/*.test.ts",
    "bench:compare": "tsx scripts/bench/compare-ts-vs-rust.ts"
  },
  "keywords": [
    "ai",
    "agents",
    "monitoring",
    "dashboard"
  ],
  "license": "ISC",
  "packageManager": "pnpm@10.29.3",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "express": "^5.2.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/cli": "^4.1.18",
    "@tauri-apps/cli": "^2.10.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.3",
    "eslint": "^10.0.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  }
}
